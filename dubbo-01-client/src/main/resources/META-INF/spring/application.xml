<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd
http://code.alibabatech.com/schema/dubbo
http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!--配置文件，dubbo 应用的元数据信息，以及注册中心等-->
    <dubbo:application name="practice-client"/>
    <!--注册中心地址-->
    <dubbo:registry address="zookeeper://192.168.1.153:2181" id="registry01" file="d:/dubbo/registry"/>
    <dubbo:registry address="zookeeper://192.168.1.115:2181" id="registry02" file="d:/dubbo/registry"/>
    <!--<dubbo:registry address="zookeeper://192.168.1.153:2181" id="registry02"/>-->
    <!--服务发现 从不同的注册中心拿到地址-->
    <dubbo:reference interface="com.mrl.emulate.api.LoginService" id="loginService" registry="registry01"/>
    <dubbo:reference interface="com.mrl.emulate.api.EmpService" id="empService" registry="registry02"/>
</beans>